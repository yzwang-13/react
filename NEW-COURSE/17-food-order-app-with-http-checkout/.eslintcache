ore/OrderContext.js",[],"/Users/yingzhengwang/Desktop/Desktop/react/relearn/react-complete-guilde/NEW-COURSE/17-food-order-app-with-http-checkout/src/index.js",[],"/Users/yingzhengwang/Desktop/Desktop/react/relearn/react-complete-guilde/NEW-COURSE/17-food-order-app-with-http-checkout/src/App.js",["108","109"],"/Users/yingzhengwang/Desktop/Desktop/react/relearn/react-complete-guilde/NEW-COURSE/17-food-order-app-with-http-checkout/src/Store/OrderContext.js",[],"/Users/yingzhengwang/Desktop/Desktop/react/relearn/react-complete-guilde/NEW-COURSE/17-food-order-app-with-http-checkout/src/components/Meals/Meals.js",["110"],["111","112"],"/Users/yingzhengwang/Desktop/Desktop/react/relearn/react-complete-guilde/NEW-COURSE/17-food-order-app-with-http-checkout/src/components/Cart/Cart.js",["113","114","115","116"],"import React, {useContext, useEffect, useRef, useState} from 'react';\nimport classes from './Cart.module.css';\nimport OrderContext from \"../../Store/OrderContext\";\nimport Backdrop from \"./Backdrop\";\nimport ModalOverlay from \"./ModalOverlay\";\n\nconst Cart = props => {\n\n    const orderContext = useContext(OrderContext);\n\n    return (\n        <div>\n            <Backdrop cancelCheckOut={orderContext.cancelCheckOut}/>\n            <ModalOverlay\n                cancelCheckOut={orderContext.cancelCheckOut}\n                totalPrice={orderContext.totalPrice}\n                orders={orderContext.orders}\n                minusAmount={orderContext.minusAmount}\n                addAmount={orderContext.addAmount}\n                checkout={orderContext.checkoutHandler}\n            />\n        </div>\n    )\n}\n\nexport default Cart;\n","/Users/yingzhengwang/Desktop/Desktop/react/relearn/react-complete-guilde/NEW-COURSE/17-food-order-app-with-http-checkout/src/components/Layout/Header/Header.js",[],"/Users/yingzhengwang/Desktop/Desktop/react/relearn/react-complete-guilde/NEW-COURSE/17-food-order-app-with-http-checkout/src/components/Meals/Meal/Meal.js",["117","118"],"import React, {useContext, useState} from 'react';\nimport classes from './Meal.module.css';\nimport OrderContext from \"../../../Store/OrderContext\";\nimport Input from \"../../UI/Input\";\n\nconst Meal = props => {\n\n    const [enteredAmount, updateEnteredAmount] = useState(1);\n    const orderContext = useContext(OrderContext);\n\n\n    const addAmountHandler = (event) => {\n        if (event.target.type === 'number') {           // coming from input\n            updateEnteredAmount(+event.target.value);\n            return\n        }\n    }\n\n    const submitNewOrder = () => {\n        orderContext.addNewOrder({id: props.id, amount: enteredAmount, name: props.name, price: props.price});\n    }\n\n    return (\n        <li className={classes.meal}>\n            <div>\n                <h3>{props.name}</h3>\n                <div className={classes.description}>{props.description}</div>\n                <div className={classes.price}>${props.price.toFixed(2)}</div>\n            </div>\n            <Input label='Amount' input={{\n                id: 'price'+'__'+`${props.id}`,\n                type: \"number\",\n                onChange: addAmountHandler,\n                value: enteredAmount,\n                min: 1,\n                step: \"any\"\n            }}/>\n            {/*<input type=\"number\" onChange={addAmountHandler} value={enteredAmount} min={1} step=\"any\"/>*/}\n            <button onClick={submitNewOrder}>+Add</button>\n        </li>\n    )\n}\n\nexport default Meal;\n","/Users/yingzhengwang/Desktop/Desktop/react/relearn/react-complete-guilde/NEW-COURSE/17-food-order-app-with-http-checkout/src/components/Layout/Header/CartButton/CartButton.js",[],"/Users/yingzhengwang/Desktop/Desktop/react/relearn/react-complete-guilde/NEW-COURSE/17-food-order-app-with-http-checkout/src/components/UI/Input.js",[],"/Users/yingzhengwang/Desktop/Desktop/react/relearn/react-complete-guilde/NEW-COURSE/17-food-order-app-with-http-checkout/src/components/Cart/Backdrop.js",[],"/Users/yingzhengwang/Desktop/Desktop/react/relearn/react-complete-guilde/NEW-COURSE/17-food-order-app-with-http-checkout/src/components/Cart/ModalOverlay.js",["119"],"import classes from \"./Cart.module.css\";\nimport React from \"react\";\nimport useFrom from \"../../hooks/use-form\";\n\nconst ModalOverlay = props => {\n\n    // useEffect(()=>{\n    //     change_form_valid_state\n    // },[all_input_isValid_denpendencies])\n\n    const {\n        enteredValue: enteredName,\n        enteredValueIsValid: nameIsValid,\n        hasError: nameHasError,\n        onBlurHandler: nameBlur,\n        onEnteredValueChange: nameChangeHandler\n    } = useFrom(value => value.trim().length !== 0);\n\n    const nameInputClass = nameHasError ? `${classes.invalid}` : \"\";\n\n    return (\n        <div className={classes.modal}>\n            {console.log(props.orders)}\n            {Object.keys(props.orders).map((key, index) => {\n                return (\n                    <div>\n                        <h3>{props.orders[key].name}</h3>\n                        <p>${props.orders[key].price}</p>\n                        <p>x {props.orders[key].amount}</p>\n                        <button onClick={() => props.minusAmount(props.orders[key].id)}>-</button>\n                        <button onClick={() => props.addAmount(props.orders[key].id)}>+</button>\n                    </div>\n                )\n            })}\n            <p>Total Amount: ${props.totalPrice}</p>\n            {Object.keys(props.orders).length >= 1 &&\n            <form onSubmit={props.checkout}>\n                <div className={nameInputClass}>\n                    <label htmlFor=\"name\">Your Name</label>\n                    <input type=\"text\" id='name'\n                           value={enteredName}\n                           onChange={nameChangeHandler}\n                           onBlur={nameBlur}\n                    />\n                </div>\n                <div>\n                    <label htmlFor=\"street\">Street</label>\n                    <input type=\"text\" id='street'/>\n                </div>\n                <div>\n                    <label htmlFor=\"postal\">Postal Code</label>\n                    <input type=\"text\" id='postal'/>\n                </div>\n                <div>\n                    <label htmlFor=\"city\">City</label>\n                    <input type=\"text\" id='city'/>\n                </div>\n                <button type='submit'>Order</button>\n            </form>}\n\n            <button onClick={props.cancelCheckOut}>Close</button>\n\n        </div>\n    )\n}\n\nexport default ModalOverlay;\n","/Users/yingzhengwang/Desktop/Desktop/react/relearn/react-complete-guilde/NEW-COURSE/17-food-order-app-with-http-checkout/src/hooks/use-form.js",["120"],"import {useEffect, useState} from \"react\";\n\nconst useFrom = (validate=()=>{}) => {\n\n    const [enteredValue, setEnteredValue] = useState('');\n    const [isTouched, setIsTouched] = useState(false);\n\n    const enteredValueIsValid = validate(enteredValue);\n    const hasError = !enteredValueIsValid && isTouched;\n    console.log(hasError)\n\n    const onEnteredValueChange = (event) => {\n        setIsTouched(true);\n        setEnteredValue(event.target.value);\n    }\n\n    const reset = () => {\n        setIsTouched(false);\n        setEnteredValue('');\n    }\n\n    const onBlurHandler = () => {\n        setIsTouched(true);\n    }\n\n    return {\n        enteredValue,\n        enteredValueIsValid,\n        hasError,\n        onBlurHandler,\n        onEnteredValueChange,\n        reset\n    }\n\n};\n\nexport default useFrom;\n","/Users/yingzhengwang/Desktop/Desktop/react/relearn/react-complete-guilde/NEW-COURSE/17-food-order-app-with-http-checkout/src/hooks/use-http.js",["121"],{"ruleId":"122","severity":1,"message":"123","line":1,"column":28,"nodeType":"124","messageId":"125","endLine":1,"endColumn":38},{"ruleId":"122","severity":1,"message":"126","line":1,"c